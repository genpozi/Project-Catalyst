
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|sw.js|manifest.json|favicon.ico|.*\\.(png|jpg|jpeg|gif|svg|json)).*)",
      "destination": "/index.html"
    }
  ]
}
